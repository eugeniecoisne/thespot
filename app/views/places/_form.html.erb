<%= simple_form_for(place) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :category, collection: Place::CATEGORIES %>
  <%= f.input :address, id: 'place_address' %>
  <%= f.input :capacity %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: 'btn btn-primary' %>
  <%= f.submit 'Cancel', class: 'btn btn-danger' %>
<% end %>
